# Supabase project URL used across supabase client helpers
NEXT_PUBLIC_SUPABASE_URL=<your-supabase-url>
# Supabase anon key for client-side requests
NEXT_PUBLIC_SUPABASE_ANON_KEY=<your-supabase-anon-key>

# Cubid API key used in shared/api/services/cubidService.ts
NEXT_PUBLIC_CUBID_API_KEY=<your-cubid-api-key>
# Cubid App ID (currently unused)
NEXT_PUBLIC_CUBID_APP_ID=<your-cubid-app-id>

# CityCoin slug used by next.config.js when constructing routes
NEXT_PUBLIC_CITYCOIN=<citycoin>
# App name served by next.config.js
NEXT_PUBLIC_APP_NAME=<app-name>

# Twilio credentials used in /api/send_otp and /api/verify_otp
accountSid=<twilio-account-sid>
authToken=<twilio-auth-token>
verifyServiceSid=<twilio-verify-service-sid>

# Additional Twilio settings for /api/sendsms
TWILLIO_API_SID=<twilio-api-sid>
TWILLIO_AUTH_TOKEN=<twilio-auth-token>
TWILLIO_SID=<twilio-sid>
TWILLIO_API_SECRET=<twilio-api-secret>
